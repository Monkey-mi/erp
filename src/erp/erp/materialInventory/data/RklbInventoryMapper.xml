<mapper namespace="erp.erp.materialInventory.data.RklbInventoryMapper">


<select id="getRklbList" parameterType="map" resultType="erp.erp.materialInventory.model.RklbInventory">
	   SELECT rklbb.rklb,   
         rklbb.lbbh  
    FROM rklbb where 1=1 
</select>
